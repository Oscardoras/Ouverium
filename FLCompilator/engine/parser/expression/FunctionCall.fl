using "Expression";


FunctionCall : (this, function, argument) \ (function ~ Expression & argument ~ Expression) |-> {
    Expression(this);
    this :~ FunctionCall;

    this->function := function;
    this->argument := argument;

    this
};

FunctionCall : (function, argument) \ (function ~ Expression & argument ~ Expression) |-> {
    FunctionCall((), function, argument)
}