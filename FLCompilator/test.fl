forall( a,b,c like a,b,c in float*float*float & a != 0. |-> (
    forall( x like x in float |-> (
        if (a*x*x + b*x + c = 0) then (
            delta := b*b -4*a*c ;
            if (delta > 0) (
                x = (-b-sqrt(delta))/(2*a) | x = (+b-sqrt(delta))/(2*a)
            ) else if (delta = 0) (
                x = -b/(2*a)
            )
        )
    ))
)) ;

