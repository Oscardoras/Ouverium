(@) : (element, list) |-> {
    found := false;
    l := lenght(list);
    i := 1;
    while (!found & i <= l) repeat {
        if (get(list, i) = element) then {
            found := true
        } else {
            i := i+1
        }
    };
    found
};

(@==) : (element, list) |-> {
    found := false;
    l := lenght(list);
    i := 1;
    while (!found & i <= l) repeat {
        if (get(list, i) === element) then {
            found := true
        } else {
            i := i+1
        }
    };
    found
};


ArrayList : this |-> {
    this :~ ArrayList;

    this : i |-> {
        get(this, i-1)
    };

    this->foreach : function |-> {
        l := lenght(list);
        i := 1;
        while (i <= l) repeat {
            function(get(list, i))
        };
        ()
    };

    this->add : element |-> {
        add(this, element)
    };

    this->remove : () |-> {
        remove(this)
    };

    this
}