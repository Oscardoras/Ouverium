{
    "name": "ouverium",
    "displayName": "Ouverium",
    "description": "VSCode support for Ouverium.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ouverium",
            "aliases": ["Ouverium", "ouverium"],
            "extensions": [".fl",".ov"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ouverium",
            "scopeName": "source.fl",
            "path": "./syntaxes/ouverium.tmLanguage.json"
        }]
    }
}