import "Type.fl";


class Iterable;
class (Iterable.Iterator);

class BidirectionalIterable extends Iterable;
class (BidirectionalIterable.Iterator) extends (Iterable.Iterator);


Iterable::(this.foreach |-> (
    (Function function) |-> {
        it := this.iterator;
        while (it.is_valid()) {
            function(it.get());
            it.next();
        };
    }
));
