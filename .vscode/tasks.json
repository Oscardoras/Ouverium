{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc générer le fichier actif",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-std=c++17",
                "-g",
                "-Wall",
                "-Wextra",
                "${workspaceFolder}/src/main.cpp",
                "${workspaceFolder}/src/parser/expression/Expression.cpp",
                "${workspaceFolder}/src/parser/Standard.cpp",
                "${workspaceFolder}/src/interpreter/Context.cpp",
                "${workspaceFolder}/src/interpreter/Interpreter.cpp",
                "${workspaceFolder}/src/interpreter/Object.cpp",
                "${workspaceFolder}/src/interpreter/Reference.cpp",
                "-o",
                "${workspaceFolder}/interpreter",
                "-pg"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Tâche générée par le débogueur."
        }
    ],
    "version": "2.0.0"
}