using "engine/parser/Standard.fl";
print("BEGIN\n");


file := InputFile("test.fl");
print("FILE LOADED\n");

str := String "";
while (file->has()) {
    s := file->read();
    str :+ (s + "\n");
};
print(str);

words := get_words(str);
print("Longueur : ", words->lenght(), "\n");
words->foreach(x |-> {
    print(x, "\n")
});
