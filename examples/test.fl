using "../engine/parser/Standard.fl";
print("BEGIN\n");


file := InputFile("examples/test2.fl");
print("FILE LOADED\n");

str := (String "");
while (file.has()) {
    s := file.read();
    print(s, "\n");
    str :+ (s + "\n");
};
print(str);

words := get_words(str);
print("Longueur : ", words.lenght(), "\n");
words.foreach(x |-> {
    print(x, "\n")
});
